{
  "name": "ragnar",
  "version": "1.0.0",
  "author": "Bohdan Donchenko",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "eslint . --ext .ts --fix",
    "e2e": "ng e2e",
    "format": "prettier-eslint --write $PWD/'src/**/*.{[jt]s, html}' && eslint . --ext .ts --fix",
    "verify": "yarn test && yarn lint && yarn build && yarn start"
